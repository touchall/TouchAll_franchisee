<?xml version="1.0" encoding="utf-8"?>
<resources>

    <declare-styleable name="HorizontalListView">
        <attr name="dividerWidth" format="dimension" />
        <attr name="android:divider" />
        <attr name="android:requiresFadingEdge" />
        <attr name="android:fadingEdgeLength" />
    </declare-styleable>

    <declare-styleable name="ViewPagerIndicator">
        <attr name="spacing" format="integer" />
        <attr name="radius" format="dimension" />
        <attr name="defaultColor" format="color" />
        <attr name="actColor" format="color" />
        <attr name="lineColor" format="color" />
    </declare-styleable>
    <declare-styleable name="CircleProgressView">
        <attr name="text" format="string" />
        <attr name="unit" format="string" />
        <attr name="showUnit" format="boolean" />
        <attr name="showValue" format="boolean" />
        <attr name="textColor" format="color" />
        <attr name="unitColor" format="color" />
        <attr name="textSize" format="dimension" />
        <attr name="spinColor" format="color" />
        <attr name="maxValue" format="dimension" />
        <attr name="barColor" format="color" />
        <attr name="barColor1" format="color" />
        <attr name="barColor2" format="color" />
        <attr name="barColor3" format="color" />
        <attr name="rimColor" format="color" />
        <attr name="rimWidth" format="dimension" />
        <attr name="barWidth" format="dimension" />
        <attr name="spinSpeed" format="dimension" />
        <attr name="fillColor" format="color" />
        <attr name="spinBarLength" format="dimension" />
        <attr name="contourColor" format="color" />
        <attr name="contourSize" format="dimension" />
        <attr name="textScale" format="dimension" />
        <attr name="unitScale" format="dimension" />
    </declare-styleable>


</resources>
